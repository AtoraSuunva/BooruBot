{
  "name": "boorubot",
  "version": "4.8.0",
  "description": "A Discord bot to search boorus",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "start:dev": "tsc --build && node --env-file=.env --enable-source-maps --trace-warnings dist/index.js",
    "start:dist": "node --env-file=.env --enable-source-maps --trace-warnings dist/index.js",
    "start:prod": "prisma migrate deploy && node --enable-source-maps dist/index.js",
    "generate": "prisma generate",
    "build": "tsc --build",
    "lint": "biome check && tsc --noEmit",
    "lint:fix": "biome check --write && prisma format && tsc --noEmit",
    "sentry:sourcemaps:inject": "sentry-cli sourcemaps inject ./dist",
    "sentry:sourcemaps:upload": "sentry-cli sourcemaps upload ./dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AtoraSuunva/BooruBot.git"
  },
  "keywords": [],
  "author": "AtoraSuunva",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AtoraSuunva/BooruBot/issues"
  },
  "homepage": "https://github.com/AtoraSuunva/BooruBot#readme",
  "packageManager": "pnpm@10.15.1",
  "engines": {
    "node": ">=22.0.0"
  },
  "dependencies": {
    "@prisma/adapter-better-sqlite3": "^6.15.0",
    "@prisma/client": "6.15.0",
    "@prisma/instrumentation": "^6.15.0",
    "@types/pluralize": "^0.0.33",
    "better-sqlite3": "^12.2.0",
    "booru": "^2.8.1",
    "discord.js": "^14.22.1",
    "dotenv": "^17.2.2",
    "env-var": "^7.5.0",
    "magic-bytes.js": "^1.12.1",
    "murmurhash": "^2.0.1",
    "pino": "^9.9.4",
    "pino-dev": "^4.0.3",
    "pluralize": "^8.0.0",
    "pretty-ms": "^9.2.0",
    "prisma": "^6.15.0",
    "sleetcord": "^3.0.0",
    "sleetcord-common": "^15.0.1",
    "string-width": "^8.1.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.3",
    "@sentry/cli": "^2.53.0",
    "@types/node": "^24.3.1",
    "typescript": "^5.9.2"
  }
}
